{
  "manifest_version": 2,

  "name": "AdBlock",
  "description": "La plus populaire des extensions Chrome, avec plus de 40 millions d'utilisateurs ! Bloque la publicit√© sur le web.",
  "version": "3.21.0",
  "browser_action": {
    "default_icon": "icon16.png"
},
  "icons": {
    "48": "icon48.png"
  },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
  "permissions": [ "http://*/*", "https://*/*" ]
}
